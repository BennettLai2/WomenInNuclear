<h2> Leader Board</h2>

<table>
<tbody>
    <thead>
        <th> </th>
        <th> Name </th>
        <th> Points </th>

    </thead>
    
    <% @accounts.each_with_index do |account, index| %>
        <tr>
        <td><%= index + 1 %></td>
        <td><%= account.first_name %> <%= account.last_name %> </td>
        <td><%= account.points %></td>
        </tr>
    <% end %>
</tbody>
</table>

<%= link_to 'Back', accounts_path %>



