<div>
    <% flash.each do |type, msg| %>
        <div>
          <%= msg %>
        </div>
    <% end %>
      
    <h2> Login </h2>
    <%= form_tag '/login' do %>
        Email:  <%= text_field_tag :email %> <br>
        Password: <%= text_field_tag :password%> <br>
        <%= submit_tag "Login" %>
    <% end %>
</div>

<%= link_to 'New Account', new_account_path %>